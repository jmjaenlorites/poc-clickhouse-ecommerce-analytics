            "targets": [
                {
                    "datasource": {
                        "type": "vertamedia-clickhouse-datasource",
                        "uid": "clickhouse"
                    },
                    "queryType": "",
                    "query": "SELECT\n    method AS \"Method\",\n    endpoint AS \"Endpoint\",\n    round(avg(response_time_ms), 2) AS \"Avg Response Time\",\n    round(quantile(0.50)(response_time_ms), 2) AS \"P50\",\n    round(quantile(0.95)(response_time_ms), 2) AS \"P95\",\n    round(quantile(0.99)(response_time_ms), 2) AS \"P99\",\n    count() AS \"Requests\"\nFROM analytics.request_metrics\nWHERE timestamp >= now() - INTERVAL 1 HOUR\nGROUP BY method, endpoint\nORDER BY \"Avg Response Time\" DESC\nLIMIT 20",
                    "refId": "A"
                }
            ],
            "title": "Response Time by Endpoint",
            "type": "table"
